<section class="about-usp" data-scroll-section>
  <div class="container">
    <div class="about-usp-wrapper">
      {% for block in section.blocks %}
        <div class="item">
          <div class="auw-head">
            <p>{{ block.settings.head }}</p>
          </div>
          <div class="auw-body">
            <p>{{ block.settings.para }}</p>
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</section>
{% schema %}
  {
    "name": "About Page Usp",
    "settings": [],
    "blocks": [
      {
        "name": "About USP",
        "type": "about-usp",
        "settings": [
          {
            "type": "text",
            "id": "head",
            "label": "Heading"
          }, {
            "type": "text",
            "id": "para",
            "label": "Paragraph"
          }
        ]
      }
    ]
  }
{% endschema %}